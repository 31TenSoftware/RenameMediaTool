<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.thirtyonetensoftware.renamemediatool.Controller">
    <Button mnemonicParsing="false" onAction="#onChooseDirectoryButtonClick" prefHeight="31.0" prefWidth="658.0"
            text="Choose Folder"/>
    <Label fx:id="mPath" wrapText="true">
        <VBox.margin>
            <Insets bottom="20.0" top="20.0"/>
        </VBox.margin>
    </Label>
    <ProgressBar fx:id="mProgressBar" prefHeight="23.0" prefWidth="658.0" progress="0.0"/>
    <Button mnemonicParsing="false" onAction="#onStopButtonClick" prefHeight="31.0" prefWidth="658.0" text="Stop">
        <VBox.margin>
            <Insets bottom="10.0" top="10.0"/>
        </VBox.margin>
    </Button>
    <TextArea fx:id="mResults" prefHeight="200.0" prefWidth="200.0" wrapText="true" VBox.vgrow="ALWAYS"/>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
